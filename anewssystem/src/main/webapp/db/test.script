CREATE MEMORY TABLE A_NEWS(ID BIGINT NOT NULL PRIMARY KEY,CONTENT LONGVARCHAR,EDITOR VARCHAR(50),HIT INTEGER,IMAGE VARCHAR(50),LINK VARCHAR(100),NAME VARCHAR(100),SOURCE VARCHAR(50),STATUS INTEGER,SUBTITLE VARCHAR(100),SUMMARY VARCHAR(255),UPDATE_DATE TIMESTAMP,CATEGORY_ID BIGINT,CONSTRAINT SYS_CT_46 UNIQUE(ID))
CREATE MEMORY TABLE A_NEWS_CATEGORY(ID BIGINT NOT NULL PRIMARY KEY,BIT_CODE BIGINT,CHAR_CODE VARCHAR(255),NAME VARCHAR(50),STATUS INTEGER,THE_SORT INTEGER,PARENT_ID BIGINT,CONSTRAINT SYS_CT_50 UNIQUE(ID),CONSTRAINT FK6567A56CAF1C28CB FOREIGN KEY(PARENT_ID) REFERENCES A_NEWS_CATEGORY(ID))
CREATE MEMORY TABLE A_NEWS_COMMENT(ID BIGINT NOT NULL PRIMARY KEY,CONTENT LONGVARCHAR,IP VARCHAR(20),NAME VARCHAR(100),STATUS INTEGER,UPDATE_DATE TIMESTAMP,USERNAME VARCHAR(50),NEWS_ID BIGINT,CONSTRAINT SYS_CT_54 UNIQUE(ID),CONSTRAINT FK5CDABC117E0115E4 FOREIGN KEY(NEWS_ID) REFERENCES A_NEWS(ID))
CREATE MEMORY TABLE A_NEWS_CONFIG(ID BIGINT NOT NULL PRIMARY KEY,CATEGORY_STRATEGY INTEGER,COMMENT_NEED_AUDIT INTEGER,COULD_COMMENT INTEGER,NEWS_NEED_AUDIT INTEGER,TEMPLATE_NAME VARCHAR(50),CONSTRAINT SYS_CT_58 UNIQUE(ID))
CREATE MEMORY TABLE A_NEWS_NEWSTAG(NEWSTAG_ID BIGINT NOT NULL,NEWS_ID BIGINT NOT NULL,PRIMARY KEY(NEWS_ID,NEWSTAG_ID),CONSTRAINT FK923EAD997E0115E4 FOREIGN KEY(NEWS_ID) REFERENCES A_NEWS(ID))
CREATE MEMORY TABLE A_NEWS_TAG(ID BIGINT NOT NULL PRIMARY KEY,NAME VARCHAR(50),THE_SORT INTEGER,CONSTRAINT SYS_CT_64 UNIQUE(ID))
CREATE MEMORY TABLE A_SECURITY_DEPT(ID BIGINT NOT NULL PRIMARY KEY,DESCN VARCHAR(200),NAME VARCHAR(50),THE_SORT INTEGER,PARENT_ID BIGINT,CONSTRAINT SYS_CT_68 UNIQUE(ID),CONSTRAINT FK33849C668034938C FOREIGN KEY(PARENT_ID) REFERENCES A_SECURITY_DEPT(ID))
CREATE MEMORY TABLE A_SECURITY_MENU(ID BIGINT NOT NULL PRIMARY KEY,DESCN VARCHAR(200),FORWARD VARCHAR(50),IMAGE VARCHAR(50),NAME VARCHAR(50),TARGET VARCHAR(50),THE_SORT INTEGER,TIP VARCHAR(50),TITLE VARCHAR(50),PARENT_ID BIGINT,CONSTRAINT SYS_CT_72 UNIQUE(ID),CONSTRAINT FK3388B3808038AAA6 FOREIGN KEY(PARENT_ID) REFERENCES A_SECURITY_MENU(ID))
CREATE MEMORY TABLE A_SECURITY_MENU_ROLE(MENU_ID BIGINT NOT NULL,ROLE_ID BIGINT NOT NULL,PRIMARY KEY(ROLE_ID,MENU_ID),CONSTRAINT FK14BCADD53D7765D1 FOREIGN KEY(MENU_ID) REFERENCES A_SECURITY_MENU(ID))
CREATE MEMORY TABLE A_SECURITY_RESOURCE(ID BIGINT NOT NULL PRIMARY KEY,DESCN VARCHAR(200),NAME VARCHAR(50),RES_STRING VARCHAR(200),RES_TYPE VARCHAR(50),CONSTRAINT SYS_CT_78 UNIQUE(ID))
CREATE MEMORY TABLE A_SECURITY_RESOURCE_ROLE(RESOURCE_ID BIGINT NOT NULL,ROLE_ID BIGINT NOT NULL,PRIMARY KEY(ROLE_ID,RESOURCE_ID),CONSTRAINT FK6FD185064E5B1FF1 FOREIGN KEY(RESOURCE_ID) REFERENCES A_SECURITY_RESOURCE(ID))
CREATE MEMORY TABLE A_SECURITY_ROLE(ID BIGINT NOT NULL PRIMARY KEY,DESCN VARCHAR(200),NAME VARCHAR(50),CONSTRAINT SYS_CT_84 UNIQUE(ID))
CREATE MEMORY TABLE A_SECURITY_ROLE_USER(ROLE_ID BIGINT NOT NULL,USER_ID BIGINT NOT NULL,PRIMARY KEY(USER_ID,ROLE_ID),CONSTRAINT FK83C6047356E5FA71 FOREIGN KEY(ROLE_ID) REFERENCES A_SECURITY_ROLE(ID))
CREATE MEMORY TABLE A_SECURITY_USER(ID BIGINT NOT NULL PRIMARY KEY,BIRTHDAY TIMESTAMP,CODE VARCHAR(50),DESCN VARCHAR(200),DUTY VARCHAR(50),EMAIL VARCHAR(100),MOBILE VARCHAR(50),PASSWORD VARCHAR(50),SEX TINYINT,STATUS TINYINT,TEL VARCHAR(50),TRUENAME VARCHAR(50),USERNAME VARCHAR(50),DEPT_ID BIGINT,CONSTRAINT SYS_CT_90 UNIQUE(ID),CONSTRAINT FK338C89EC6176F411 FOREIGN KEY(DEPT_ID) REFERENCES A_SECURITY_DEPT(ID))
