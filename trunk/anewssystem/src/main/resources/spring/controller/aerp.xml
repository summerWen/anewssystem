<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<!-- 小型企业erp -->
<beans default-lazy-init="false" default-autowire="byName">
    <bean class="anni.aerp.web.ErpMaterialController" scope="request">
        <property name="entityDao" ref="erpMaterialManager"/>
    </bean>
    <bean class="anni.aerp.web.ErpProductController" scope="request">
        <property name="entityDao" ref="erpProductManager"/>
    </bean>
    <bean class="anni.aerp.web.ErpCustomerController" scope="request">
        <property name="entityDao" ref="erpCustomerManager"/>
    </bean>
    <bean class="anni.aerp.web.ErpRationController" scope="request">
        <property name="entityDao" ref="erpRationManager"/>
    </bean>
    <bean class="anni.aerp.web.ErpOrderController" scope="request">
        <property name="entityDao" ref="erpOrderManager"/>
    </bean>
    <bean class="anni.aerp.web.ErpController" scope="request"/>

    <bean class="anni.aerp2.web.Erp2SupplierController" scope="request">
        <property name="entityDao" ref="erp2SupplierManager"/>
    </bean>
    <bean class="anni.aerp2.web.Erp2ProductController" scope="request">
        <property name="entityDao" ref="erp2ProductManager"/>
    </bean>
    <bean class="anni.aerp2.web.Erp2BidController" scope="request">
        <property name="entityDao" ref="erp2BidManager"/>
    </bean>
    <bean class="anni.aerp2.web.Erp2InviteBidController" scope="request">
        <property name="entityDao" ref="erp2InviteBidManager"/>
    </bean>
    <bean class="anni.aerp2.web.Erp2SuccessBidController" scope="request">
        <property name="entityDao" ref="erp2SuccessBidManager"/>
    </bean>
    <bean class="anni.aerp2.web.Erp2BuyOrderController" scope="request">
        <property name="entityDao" ref="erp2BuyOrderManager"/>
    </bean>
    <bean class="anni.aerp2.web.Erp2BuyOrderInfoController" scope="request">
        <property name="entityDao" ref="erp2BuyOrderInfoManager"/>
    </bean>
    <bean class="anni.aerp2.web.Erp2BuyContractController" scope="request">
        <property name="entityDao" ref="erp2BuyContractManager"/>
    </bean>
</beans>
