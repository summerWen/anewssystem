CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE A_NEWS(ID BIGINT NOT NULL PRIMARY KEY,SUBTITLE VARCHAR(100),LINK VARCHAR(100),HIT INTEGER,SUMMARY VARCHAR(255),UPDATE_DATE TIMESTAMP,NAME VARCHAR(100),CONTENT LONGVARCHAR,SOURCE VARCHAR(50),IMAGE VARCHAR(50),EDITOR VARCHAR(50),STATUS INTEGER,CATEGORY_ID BIGINT,CONSTRAINT SYS_CT_75 UNIQUE(ID))
CREATE MEMORY TABLE A_NEWS_CATEGORY(ID BIGINT NOT NULL PRIMARY KEY,THE_SORT INTEGER,CODE BIGINT,NAME VARCHAR(50),STATUS INTEGER,PARENT_ID BIGINT,CONSTRAINT SYS_CT_79 UNIQUE(ID),CONSTRAINT FK6567A56CAF1C28CB FOREIGN KEY(PARENT_ID) REFERENCES A_NEWS_CATEGORY(ID))
CREATE MEMORY TABLE A_NEWS_COMMENT(ID BIGINT NOT NULL PRIMARY KEY,USERNAME VARCHAR(50),UPDATE_DATE TIMESTAMP,IP VARCHAR(20),NAME VARCHAR(100),CONTENT LONGVARCHAR,STATUS INTEGER,NEWS_ID BIGINT,CONSTRAINT SYS_CT_83 UNIQUE(ID),CONSTRAINT FK5CDABC117E0115E4 FOREIGN KEY(NEWS_ID) REFERENCES A_NEWS(ID))
CREATE MEMORY TABLE A_NEWS_CONFIG(ID BIGINT NOT NULL PRIMARY KEY,NEWS_NEED_AUDIT INTEGER,COMMENT_NEED_AUDIT INTEGER,COULD_COMMENT INTEGER,CONSTRAINT SYS_CT_87 UNIQUE(ID))
CREATE MEMORY TABLE A_NEWS_NEWSTAG(NEWSTAG_ID BIGINT NOT NULL,NEWS_ID BIGINT NOT NULL,PRIMARY KEY(NEWS_ID,NEWSTAG_ID),CONSTRAINT FK923EAD997E0115E4 FOREIGN KEY(NEWS_ID) REFERENCES A_NEWS(ID))
CREATE MEMORY TABLE A_NEWS_TAG(ID BIGINT NOT NULL PRIMARY KEY,THE_SORT INTEGER,NAME VARCHAR(50),CONSTRAINT SYS_CT_93 UNIQUE(ID))
ALTER TABLE A_NEWS ADD CONSTRAINT FK744A3C118F5E23F7 FOREIGN KEY(CATEGORY_ID) REFERENCES A_NEWS_CATEGORY(ID)
ALTER TABLE A_NEWS_NEWSTAG ADD CONSTRAINT FK923EAD9963BB8EF0 FOREIGN KEY(NEWSTAG_ID) REFERENCES A_NEWS_TAG(ID)
