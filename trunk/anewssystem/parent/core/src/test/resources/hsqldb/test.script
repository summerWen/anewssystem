CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE ARTICLE(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TITLE VARCHAR(255),CATEGORY_ID INTEGER)
CREATE MEMORY TABLE CATEGORY(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(255))
CREATE MEMORY TABLE MENU(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,SEQ INTEGER,TIP VARCHAR(255),FORWARD VARCHAR(255),NAME VARCHAR(255),TITLE VARCHAR(255),IMAGE VARCHAR(255),TARGET VARCHAR(255),DESCRIPTION VARCHAR(255),PARENT_ID INTEGER,CONSTRAINT FK24897F3F311C6A FOREIGN KEY(PARENT_ID) REFERENCES MENU(ID),CONSTRAINT FK24897FC3CA78B5 FOREIGN KEY(ID) REFERENCES MENU(ID))
CREATE MEMORY TABLE COMMENT(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,EMAIL VARCHAR(255),PUBLISHDATE TIMESTAMP,CONTENT VARCHAR(255),TITLE VARCHAR(255),ARTICLE_ID INTEGER,CONSTRAINT FK9BDE863F66981DF FOREIGN KEY(ARTICLE_ID) REFERENCES ARTICLE(ID))
CREATE MEMORY TABLE SS_USERS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL,PASSWD VARCHAR(255) NOT NULL,NAME VARCHAR(80) NOT NULL,EMAIL VARCHAR(255),STATUS INTEGER NOT NULL,DESCN VARCHAR(255),CONSTRAINT PK_USERS PRIMARY KEY(ID))
CREATE MEMORY TABLE SS_ROLES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL,NAME VARCHAR(80) NOT NULL,DESCN VARCHAR(255),CONSTRAINT PK_ROLES PRIMARY KEY(ID))
CREATE MEMORY TABLE SS_USER_ROLE(USER_ID INTEGER NOT NULL,ROLE_ID INTEGER NOT NULL,CONSTRAINT PK_USER_ROLE PRIMARY KEY(USER_ID,ROLE_ID),CONSTRAINT FK_USER_ROLE_1 FOREIGN KEY(USER_ID) REFERENCES SS_USERS(ID),CONSTRAINT FK_USER_ROLE_2 FOREIGN KEY(ROLE_ID) REFERENCES SS_ROLES(ID))
CREATE MEMORY TABLE SS_RESOURCES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL,NAME VARCHAR(80) NOT NULL,RES_TYPE VARCHAR(20) NOT NULL,RES_STRING VARCHAR(255) NOT NULL,DESCN VARCHAR(255),CONSTRAINT PK_RESOURCES PRIMARY KEY(ID))
CREATE MEMORY TABLE SS_ROLE_RESC(ROLE_ID INTEGER NOT NULL,RESC_ID INTEGER NOT NULL,CONSTRAINT PK_ROLE_RESC PRIMARY KEY(ROLE_ID,RESC_ID),CONSTRAINT FK_ROLE_RESC_1 FOREIGN KEY(ROLE_ID) REFERENCES SS_ROLES(ID),CONSTRAINT FK_ROLE_RESC_2 FOREIGN KEY(RESC_ID) REFERENCES SS_RESOURCES(ID))
CREATE MEMORY TABLE SS_ROLE_MENU(ROLE_ID INTEGER NOT NULL,MENU_ID INTEGER NOT NULL,PRIMARY KEY(MENU_ID,ROLE_ID),CONSTRAINT FK27DEB46915DE6C35 FOREIGN KEY(ROLE_ID) REFERENCES SS_ROLES(ID),CONSTRAINT FK27DEB469FC6FD795 FOREIGN KEY(MENU_ID) REFERENCES MENU(ID))
ALTER TABLE ARTICLE ADD CONSTRAINT FK379164D63DE729B5 FOREIGN KEY(CATEGORY_ID) REFERENCES CATEGORY(ID)
ALTER TABLE ARTICLE ALTER COLUMN ID RESTART WITH 2
ALTER TABLE CATEGORY ALTER COLUMN ID RESTART WITH 2
ALTER TABLE MENU ALTER COLUMN ID RESTART WITH 14
ALTER TABLE COMMENT ALTER COLUMN ID RESTART WITH 1
ALTER TABLE SS_USERS ALTER COLUMN ID RESTART WITH 23
ALTER TABLE SS_ROLES ALTER COLUMN ID RESTART WITH 4
ALTER TABLE SS_RESOURCES ALTER COLUMN ID RESTART WITH 19
